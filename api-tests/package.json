{
  "name": "party-management-api-tests",
  "version": "1.0.0",
  "description": "党建管理系统API接口测试",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:auth": "jest --testPathPattern=auth",
    "test:members": "jest --testPathPattern=members",
    "test:organizations": "jest --testPathPattern=organizations",
    "test:activities": "jest --testPathPattern=activities",
    "test:fees": "jest --testPathPattern=fees",
    "test:statistics": "jest --testPathPattern=statistics",
    "test:system": "jest --testPathPattern=system",
    "test:smoke": "jest --testNamePattern='smoke'",
    "test:integration": "jest --testNamePattern='integration'",
    "test:performance": "jest --testNamePattern='performance'",
    "test:security": "jest --testNamePattern='security'",
    "test:parallel": "jest --maxWorkers=4",
    "test:serial": "jest --runInBand",
    "test:verbose": "jest --verbose",
    "test:silent": "jest --silent",
    "report": "open test-results/coverage/lcov-report/index.html",
    "clean": "rm -rf test-results node_modules",
    "setup": "npm install"
  },
  "keywords": [
    "jest",
    "api-testing",
    "integration-testing",
    "supertest",
    "党建管理系统"
  ],
  "author": "党建管理系统开发团队",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "axios": "^1.6.0",
    "chai": "^5.3.1",
    "faker": "^6.6.6",
    "jest": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/party-management-system.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/party-management-system/issues"
  },
  "homepage": "https://github.com/your-org/party-management-system#readme"
}
